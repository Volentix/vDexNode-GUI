{
  "name": "volentixnode",
  "version": "0.4.0",
  "description": "Volentix Node",
  "productName": "Volentix Node",
  "cordovaId": "org.cordova.volentixnode",
  "author": "Volentix Labs <volentix.io>",
  "private": true,
  "homepage": "https://github.com/Volentix/vdexnode-gui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Volentix/vdexnode-gui.git"
  },
  "bugs": {
    "url": "https://github.com/Volentix/vdexnode-gui/issues"
  },
  "scripts": {
    "dev": "quasar dev -m electron",
    "build": "quasar build -m electron",
    "publish": "quasar build -m electron -P always",
    "build-linux": "quasar build -m electron --target linux",
    "publish-linux": "quasar build -m electron --target linux -P always",
    "build-all": "quasar build -m electron --target all",
    "publish-all": "quasar build -m electron --target all -P always",
    "bump": "npm version patch",
    "lint": "eslint --ext .js,.vue src",
    "docs": "jsdoc -c jsdoc.json",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.6.0",
    "@scatterjs/core": "^2.7.47",
    "@scatterjs/eosjs2": "^1.5.33",
    "axios": "^0.18.1",
    "electron-log": "^3.0.9",
    "electron-store": "^5.1.1",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "^4.2.2",
    "electron-util": "^0.12.3",
    "eosjs": "^20.0.3",
    "eosjs-ecc": "^4.0.7",
    "fast-json-patch": "^3.0.0-1",
    "leaflet": "^1.6.0",
    "node-fetch": "^2.6.0",
    "node-notifier": "^6.0.0",
    "quasar": "^1.9.12",
    "stream-http": "^3.1.0",
    "streamdataio-js-sdk": "^2.0.2",
    "vue-chat-widget": "^0.3.1",
    "vue-i18n": "^8.15.4"
  },
  "devDependencies": {
    "@quasar/app": "^1.6.0",
    "@quasar/quasar-app-extension-dotenv": "^1.0.0",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "devtron": "^1.4.0",
    "electron": "^6.1.9",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-utils": "^1.4.3",
    "jsdoc": "^3.6.3",
    "jsdoc-vuejs": "^3.0.8",
    "minami": "^1.2.3",
    "raw-loader": "^4.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
